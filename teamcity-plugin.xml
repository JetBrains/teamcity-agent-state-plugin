    <download-url>https://github.com/JetBrains/teamcity-agent-state-plugin</download-url>
    <email>mikhail@pilin.name</email>
    <vendor>
      <name>JetBrains</name>
      <url>https://www.jetbrains.com/</url>
      <logo>https://www.jetbrains.com/company/docs/logo_jetbrains.png</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" allow-runtime-reload="true" />
</teamcity-plugin>
